{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "paths": {
      "helper-lib": [
        "dist/helper-lib/helper-lib",
        "dist/helper-lib"
      ]
    },
    "experimentalDecorators": true,
    "module": "ES2020",
    "allowJs": true,
    "moduleResolution": "node",
    "importHelpers": false,
    "target": "ES2015",
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
